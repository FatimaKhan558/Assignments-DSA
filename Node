#include <iostream>
using namespace std ;
class Node
{
    private:
    int data;
    Node* Next;
    public:
    Node(int data, Node* Next=NULL)
    {
        this-> data = data;
        Next = NULL;
    }
    void SetData (int data)
    {
        this -> data = data;
    }
    void SetNext (Node* Next)
    {
        this-> Next = Next;
    }
    int GetData()
    {
        return this-> data;
    }
    Node* GetNext()
    {
        return this->Next;
    }
};
int main()
    {
          Node* head = new Node(2);
		Node* n2 = new Node(3);
		Node* n3 = new Node(4);
		Node* n4 = new Node(5);
		Node* n5 = new Node(6);
		Node* n6 = new Node(7);
		Node* n7 = new Node(8);
		Node* n8 = new Node(9);
		Node* n9 = new Node(10);
		Node* n10 = new Node(11);
		Node* n11 = new Node(12);
		Node* n12 = new Node(13);
		Node* n13 = new Node(14);
		Node* n14 = new Node(15);
		Node* n15 = new Node(16);
		Node* n16 = new Node(17);
		Node* n17 = new Node(18);
		Node* n18 = new Node(19);
		Node* n19 = new Node(20);
		Node* n20 = new Node(21);
		
        return 0;
}
