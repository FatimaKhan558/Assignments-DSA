#include <iostream>
using namespace std ;
class Node
{
    private:
    int data;
    Node* Next;
    public:
    Node(int data, Node* Next=NULL)
    {
        this-> data = data;
        Next = NULL;
    }
    void SetData (int data)
    {
        this -> data = data;
    }
    void SetNext (Node* Next)
    {
        this-> Next = Next;
    }
    int GetData()
    {
        return this-> data;
    }
    Node* GetNext()
    {
        return this->Next;
    }
};
int main()
    {
        Node n1(2);
        return 0;
}
